.table > thead > tr > th, .table > tbody > tr > td{border:none;}
.table thead th span{display: block; padding: 10px 0; background: #e5f2eb}
.table tbody td span{display: block; padding: 10px 0; background: #e9e9e9}
.selectopt{width: 100%; background: #e5f2eb; height: 36px;padding: 5px 0}
.selectopt select{width: 100%;padding-right: 20px; background: transparent; border:none; text-align: center;-moz-appearance: none;-webkit-appearance: none;-o-appearance: none;-ms-appearance: none;appearance: none; background:url(../images/drp-arrow.png) no-repeat right 5px center; }
.opacity{opacity: 0; color: transparent;}
.sitemap ul ul{padding-left: 10px}
.inactive{background: grey;}
/*.active{background: green;}*/
#prev, #next{width: auto; padding: 5px;}
.paginationBtn{ display: inline-block;}
.paginationBtn li{display: inline-block;width: 35px;background: #fff;margin: 0 5px;cursor: pointer;box-shadow: 0px 0px 5px #909090 inset;border-radius: 5px;line-height: 35px;color: #8d8d8d;}
.paginationBtn li.active{background: green; color: #fff}
.tableBtn .customBtn{width: 100%; padding: 8px 5px 9px}
.nopad{padding: 0}
.popupContent{position: absolute; top: 50%; left: 50%; transform: translate(-50%, -50%); -webkit-transform: translate(-50%, -50%); z-index: 2; background: #fff; padding: 5px;}
.InnerCant{display: none;border: 3px solid green;padding: 20px;}
.table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
    padding: 2px;}
 .table thead th span {padding: 5px 0;}
.addDash::after{content: '-'; position: absolute; right: 0}
.myActTitle{ float: left;  width: 30%;padding:10px 10px 10px 0; text-align: center;}
.myActDetails{ float: right; width: 70%;padding:10px 0px 10px 10px; position: relative; margin-bottom: 0}
.myActTitle span{padding: 10px;background: #e5f2eb; display: block; font-weight: bold;max-width:150px;}
.myActDetails span{padding: 10px;background: #e9e9e9; display: block;}
.fullWidth .myActTitle{width: 14.5%}
.fullWidth .myActDetails{width: 85.5%}
.clickingBtn{position: absolute;top: 20px; right: 10px; text-decoration: underline;}
.fullWidth .attached{width: 50%; float: left;}
.attached input{width: 100%;padding: 10px;border: none;}
.fullWidth .forBtn{width: 34%}
.fullWidth .forBtn #uploadBtn{width: 100%; right: 0; top: auto; position: absolute; padding: 10px;opacity: 0; cursor: pointer;}
.myActDetails input, .myActDetails select, .myActDetails textarea{background-color:#e9e9e9; padding-left: 10px }
.myActDetails input[disabled]{background-color: #f5f5f5; color:#999;}
.myActDetails select[disabled]{background: #f5f5f5;color:#999;}
.mobsetBtn .customBtn{margin-top: 10px;}
.middleSecfont{font-size: 24px}
.bOfd .innerTab, .leagueSec .innerTab { padding: 30px 0 0;}
.bOfd .innerTab span, .leagueSec .innerTab span {font-size: 20px;}
.setinCenter{float: none; display:block; margin: 0 auto}
.msnStmnt p { font-size: 20px;}
.tablesMob .table > tbody > tr > td::before{display: none;}
    .tablesMob .table > tbody > tr > td span{vertical-align: top}
    .tablesMob .table > tbody > tr > td span.dptadded{display: none}
    .tablesMob .dptadded::before{display: none !important;}
.psluserpic ul{margin-top: 72px;}
.psluserpic ul li{min-height: 98px;}
.psluserpiclft{padding-right: 0 !important; padding-left: 4px;}
.psluserpicrgt{padding-left:0 !important; padding-right: 4px;}
.teamSec .innerTab{background: none; padding: 0; max-height: none}
.teamSec .txtname{font-size: 25px}
.teamSec .nofloatblock{margin-right: -4px}
.teamSec .globleHeading.text-center{text-align: center;}
.inlineBlck{display: inline-block; vertical-align: middle; float: none}
.setPosabs{position: absolute; right: 0}
.headingMid{position: absolute; left: 0; right: 0;top: 50%; margin: auto; text-align: center; transform: translateY(-50%); -webkit-transform: translateY(-50%);}
.pos-relative{position: relative;}
.setonBtm{position: absolute;left: 50%; bottom: 0; margin:auto; transform: translateX(-50%); -webkit-transform: translateX(-50%); padding-left: 5px;width: auto;padding-right: 5px; padding-left: 5px;min-width: 160px }
.teambanner{background: #000; min-height: 600px; padding: 150px 0 0}
.teambanner .bannerMidSec{width: 100%; text-align: center;}
.tmSocialIcn{width: 82px; height: 82px;margin: 5px; display: inline-block;}
.facbookIcon{background: url(../images/socialicon.png) no-repeat left center; }
.twitterIcon{background: url(../images/socialicon.png) no-repeat right center;}
.socilaSec{padding: 30px 0 0;}
