@media only screen and (max-width: 1199px) {
.myActTitle{width: 34%}
.myActDetails{width:66%;}
.fullWidth .myActTitle{width: 16.5%}
.fullWidth .myActDetails{width: 83.5%}
.fullWidth .attached{width: 50.5%}
.fullWidth .forBtn{width: 32%}
}
@media only screen and (max-width: 992px) {
.sch-livQua, .sch-onQua, .draft{padding-top: 10px}
.table > thead > tr > th{display: none; padding:10px 0}
.sch-livQua .table > thead > tr > th:first-child{display: block; width: 100%}
.table > tbody > tr > td{display: block; text-align: left; padding:0; border-bottom: 1px solid #ccc;}
.table > tbody > tr:nth-child(odd){background: #e9e9e9}
.table > tbody > tr > td span{display:inline-block; width: 50%;padding: 10px; text-align: left; background:transparent;}
.table > tbody > tr > td::before{content: ''; width: 50%;display:none;padding: 10px; font-weight: bold; vertical-align: middle;}
.table > tbody > tr > td span::before{content:':'; position: relative;left: -15px; font-weight: bold;display:inline-block}
.table > tbody > tr > td span.dptadded::before{display:none;}

.sch-livQua .table > tbody > tr > td:first-child::before{content: 'City';}
.sch-livQua .table > tbody > tr > td:nth-child(2)::before{content: 'Venue';}
.sch-livQua .table > tbody > tr > td:nth-child(3)::before{content: 'Tournament Name';}
.sch-livQua .table > tbody > tr > td:nth-child(4)::before{content: 'Date';}
.sch-livQua .table > tbody > tr > td:nth-child(5)::before{content: 'Time';}
.sch-livQua .table > tbody > tr > td:nth-child(6)::before{content: 'Entry Criteria';}
.sch-livQua .table > tbody > tr > td:nth-child(7)::before{content: 'Winnings';}
.sch-onQua .table > tbody > tr > td:nth-child(1)::before{content: 'Tournament Name';}
.sch-onQua .table > tbody > tr > td:nth-child(2)::before{content: 'Date';}
.sch-onQua .table > tbody > tr > td:nth-child(3)::before{content: 'Time';}
.sch-onQua .table > tbody > tr > td:nth-child(4)::before{content: 'Entry Criteria';}
.sch-onQua .table > tbody > tr > td:nth-child(5)::before{content: 'Winnings';}

.live_leader .table > thead > tr > th:last-child{display: block; width: 100%}
.live_leader .table > tbody > tr > td:first-child::before{content: 'Rank';}
.live_leader .table > tbody > tr > td:nth-child(2)::before{content: 'Username';}
.live_leader .table > tbody > tr > td:nth-child(3)::before{content: 'Leaderboard Points';}
.live_leader .table > tbody > tr > td:nth-child(4)::before{content: 'City';}
.online-board .table > tbody > tr > td:first-child::before{content:'Rank';}
.online-board .table > tbody > tr > td:nth-child(2)::before{content:'Adda52 Username';}
.online-board .table > tbody > tr > td:nth-child(3)::before{content:'Leaderboard Points';}

.myAccount{font-size: 14px; }
.myActTitle span{font-weight: normal; padding: 10px 0}
.tablesMob .table > tbody > tr > td span.dptadded{display: inline-block}
.psluserpic{display: none}
.setPosabs{position: relative;}


}
@media only screen and (max-width: 767px) {
.myActTitle, .fullWidth .myActTitle{width: 30%; }
.myActTitle span{max-width: none;min-width: 110px;}
.myActDetails, .fullWidth .myActDetails{width:70%;}
.fullWidth .attached{width: 70%}
.fullWidth .forBtn{width: 70%}
.inlineBlck{margin: 15px 0}
.teamSec .globleHeading { text-align: center; padding: 20px 0}
.headingMid{position: relative;left: auto; right: auto;top: auto;transform: translateY(0); -webkit-transform: translateY(0);padding: 20px 0;}
.customBtn{max-width: 100%; min-width: auto; width: 100%}
.player-appli .customBtn{width:164px;}
.teamSec .nofloatblock{margin-right: 0}
.teambanner{padding: 50% 0 10px}
.teambanner .bannerMidSec{max-width: 350px; margin: 0 auto;}
.teamDetails{margin-bottom: 10px }
}
@media only screen and (max-width: 640px) {
.paginationBtn{display: none}
.popupContent{width: 90%}
.popupContent p{font-size: 14px}

}
@media only screen and (max-width: 480px) {
.teamSec .innerTab .customBtn{font-size: 12px}	
}
@media only screen and (max-width: 420px) {
	.myActTitle, .fullWidth .myActTitle{width: 40%; }
	.myActDetails, .fullWidth .myActDetails{width:60%;}
	.myActDetails input, .myActDetails select, .myActDetails textarea{font-size: 12px}
	.myActTitle span, .myActDetails span{font-size: 12px}
	.myActDetails select{ padding:10px; background-position: right 5px center; background-size: 16px 10px;   }
	.mobsetBtn .customBtn{font-size: 14px !important; margin-top: 10px; padding: 8px 0 }

}